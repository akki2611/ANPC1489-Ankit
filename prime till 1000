//WAP to accept ano from user and check it is prime opr not

import java.util.*;
class prime_till_1000
{
	public static void main(String args[])
	{
		int num,i,j;
		for(j=2;j<=1000;j++)
		{
			int flag=0;
			for(i=2;i<j/2;i++)
			{
				if(j%i==0)
				{
					flag=1;
					break;
				}
			}
			if(flag==0)
			{
				System.out.println(j+"\t");
			}
		}	
	}
}
